<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="924.5" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.HistorialController">
   <children>
      <TableView fx:id="taulaHistorial" layoutX="-1.0" prefHeight="350.0" prefWidth="924.5">
         <columns>
            <TableColumn prefWidth="96.0" text="ID_ESPAI">
               <cellValueFactory>
                  <PropertyValueFactory property="idEspai" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="96.0" text="ID_PLANTA">
               <cellValueFactory>
                  <PropertyValueFactory property="idPlanta" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="0.0" prefWidth="144.0" text="ID_HABITACIO">
               <cellValueFactory>
                  <PropertyValueFactory property="idHabitacio" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="0.0" prefWidth="155.0" text="NOM_IAIO">
               <cellValueFactory>
                  <PropertyValueFactory property="nomIaio" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="0.0" prefWidth="144.0" text="ID_IAIO">
               <cellValueFactory>
                  <PropertyValueFactory property="idIaio" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="0.0" prefWidth="144.0" text="DATA_ENTRADA">
               <cellValueFactory>
                  <PropertyValueFactory property="dataEntrada" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn minWidth="0.0" prefWidth="144.0" text="DATA_SORTIDA">
               <cellValueFactory>
                  <PropertyValueFactory property="dataSortida" />
               </cellValueFactory>
            </TableColumn>
         </columns>
      </TableView>
      <Button layoutX="300.0" layoutY="360.0" mnemonicParsing="false" onAction="#tornaPrincipal" text="Tornar" />
   </children>
</AnchorPane>
