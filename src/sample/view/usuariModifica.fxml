      <Button fx:id="veurePass" layoutX="500.0" layoutY="210.0" mnemonicParsing="false" onAction="#showPass" text="Mostra" />
      <Label layoutX="200.0" layoutY="100.0" text="Id:" />
      <Label layoutX="200.0" layoutY="130.0" text="Nom:" />
      <Label layoutX="200.0" layoutY="210.0" text="Password:" />
      <Label layoutX="200.0" layoutY="250.0" text="Rol:" />
      <Label layoutX="132.0" layoutY="75.0" text="Introudeix l'Id de l'usuari:" />
      <Label layoutX="203.0" layoutY="22.0" text="MODIFICAR USUARI">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</Pane>
