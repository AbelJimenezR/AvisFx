      <PasswordField fx:id="pass_hidden" layoutX="310.0" layoutY="210.0" onKeyReleased="#setPass" />
      <TextField fx:id="pass" layoutX="310.0" layoutY="210.0" visible="false" />
      <ChoiceBox fx:id="rol" layoutX="310.0" layoutY="245.0" prefWidth="170.0" />
      <Button layoutX="347.0" layoutY="325.0" mnemonicParsing="false" onAction="#optBotons" text="Tornar" />
